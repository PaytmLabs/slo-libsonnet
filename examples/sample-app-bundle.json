   "recordingrule": {
      "expr": "sum(rate(http_requests_total{namespace=\"default\",job=\"fooapp\"}[10m])) BY (code)\n",
      "labels": {
         "quantile": "0.99"
      },
      "record": "code:http_requests_total:rate:sum"
   }
}
