                  },
                  "targets": [
                     {
                        "expr": "http_request_duration_seconds:histogram_quantile{quantile=\"0.99\"}",
                        "format": "time_series"
                     }
                  ],
                  "title": "P99 Latency",
                  "type": "gauge"
               },
               {
                  "dashLength": 10,
                  "dashes": false,
                  "datasource": "$datasource",
                  "fill": 2,
                  "gridPos": { },
                  "id": 3,
                  "lines": true,
                  "linewidth": 1,
                  "paceLength": 10,
                  "pointradius": 2,
                  "points": false,
                  "targets": [
                     {
                        "expr": "max(http_request_duration_seconds:histogram_quantile) by (quantile)",
                        "legendFormat": "{{ quantile }}"
                     }
                  ],
                  "thresholds": [
                     {
                        "colorMode": "warning",
                        "fill": true,
                        "line": true,
                        "op": "gt",
