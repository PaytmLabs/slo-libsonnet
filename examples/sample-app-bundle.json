{
   "alerts": [
      {
         "expr": "http_request_duration_seconds:histogram_quantile > 0.500\n",
         "for": "5m",
         "labels": {
            "severity": "warning"
         }
      },
      {
         "expr": "http_request_duration_seconds:histogram_quantile > 1.000\n",
         "for": "5m",
         "labels": {
            "severity": "critical"
         }
      },
      {
         "expr": "code:http_requests_total:rate:sum{code!~\"2..\"} * 100 / code:http_requests_total:rate:sum > 5\n",
         "for": "5m",
         "labels": {
            "severity": "warning"
         }
      },
      {
         "expr": "code:http_requests_total:rate:sum{code!~\"2..\"} * 100 / code:http_requests_total:rate:sum > 10",
         "for": "5m",
         "labels": {
            "severity": "critical"
         }
      }
   ],
   "dashboard": {
      "__inputs": [ ],
      "__requires": [ ],
      "annotations": {
         "list": [ ]
      },
      "editable": false,
      "gnetId": null,
      "graphTooltip": 0,
      "hideControls": false,
      "id": null,
      "links": [ ],
      "refresh": "",
      "rows": [
         {
            "collapse": false,
            "collapsed": false,
            "panels": [
               {
                  "datasource": "$datasource",
                  "gridPos": { },
                  "id": 2,
                  "options": {
                     "maxValue": "1.5",
                     "minValue": 0,
                     "thresholds": [
                        {
                           "color": "green",
                           "index": 0,
                           "value": null
                        },
                        {
                           "color": "#EAB839",
                           "index": 1,
                           "value": 0.5
                        },
                        {
                           "color": "red",
                           "index": 2,
                           "value": 1
                        }
                     ],
                     "valueOptions": {
                        "decimals": null,
                        "stat": "last",
